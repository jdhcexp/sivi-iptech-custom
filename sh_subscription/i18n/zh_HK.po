#. module: sh_subscription
#: model:mail.template,body_html:sh_subscription.sh_subscription_customer_email
msgid ""
"<div style=\"margin: 0px; padding: 0px;\">\n"
"\t        <table border=\"0\" width=\"100%\" cellpadding=\"0\" bgcolor=\"#ededed\" style=\"padding: 20px; background-color: #ededed; border-collapse:separate;\">\n"
"\t          <tbody>\n"
"\t\n"
"\t            <!-- HEADER -->\n"
"\t            <tr>\n"
"\t              <td align=\"center\" style=\"min-width: 590px;\">\n"
"\t                <table width=\"590\" border=\"0\" cellpadding=\"0\" bgcolor=\"#875A7B\" style=\"min-width: 590px; background-color: rgb(135,90,123); padding: 20px; border-collapse:separate;\">\n"
"\t                  <tr>\n"
"\t                    <td valign=\"middle\">\n"
"\t                      <span style=\"font-size:20px; color:white; font-weight: bold;\">\n"
"\t                        <strong>\n"
"\t                          \n"
"\t                          <t t-out=\"object.name\"/>\n"
"\t                          \n"
"\t                        </strong>\n"
"\t                      </span>\n"
"\t                    </td>\n"
"\t                    <td valign=\"middle\" align=\"right\">\n"
"\t                      <img src=\"/logo.png?company={{object.sh_company_id.id}}\" style=\"padding: 0px; margin: 0px; height: auto; width: 80px;\" alt=\"{{object.sh_company_id.name}}\"/>\n"
"\t                    </td>\n"
"\t                  </tr>\n"
"\t                </table>\n"
"\t              </td>\n"
"\t            </tr>\n"
"\t\n"
"\t            <!-- CONTENT -->\n"
"\t            <tr>\n"
"\t              <td align=\"center\" style=\"min-width: 590px;\">\n"
"\t                <table width=\"590\" border=\"0\" cellpadding=\"0\" bgcolor=\"#ffffff\" style=\"min-width: 590px; background-color: rgb(255, 255, 255); padding: 20px; border-collapse:separate;\">\n"
"\t                  <tbody>\n"
"\t                    <td valign=\"top\" style=\"font-family:Arial,Helvetica,sans-serif; color: #555; font-size: 14px;\">\n"
"\t                      <p style=\"margin: 0px; padding: 0px; font-size: 13px;\">\n"
"\t                        Dear\n"
"\t                        <b><t t-out=\"object.sh_partner_id.name\"/></b>\n"
"\t                        <br/>\n"
"\t                        <br/>\n"
"\t                        <t t-if=\"object.state == 'in_progress' and object.sh_renewed == True\">\n"
"\t\t\t\t\t\t\t\t<!-- <t t-if=\"object.sh_renewed == True\"> -->\n"
"\t\t\t\t\t\t\t\t\tYour subscription <b><t t-out=\"object.name\"/></b> is renewed.\n"
"\t\t\t\t\t\t\t\t<!-- </t> -->\n"
"\t\t\t\t\t\t\t</t>\n"
"\t\t\t\t\t\t\t<t t-else=\"\">\n"
"\t\t\t\t\t\t\t\tYour subscription <b><t t-out=\"object.name\"/></b> is activated.\n"
"\t\t\t\t\t\t\t</t>\n"
"\t\t                    \n"
"\t\t                    <t t-if=\"object.state == 'renewed'\">\n"
"\t\t                        Your subscription <b><t t-out=\"object.name\"/></b> has been renewed.\n"
"\t\t                    </t>\n"
"\t\t                    <t t-if=\"object.state == 'expire'\">\n"
"\t\t                        Your subscription <b><t t-out=\"object.name\"/></b> has been expired.\n"
"\t\t                    </t>\n"
"\t\t                    <t t-if=\"object.state == 'cancel'\">\n"
"\t\t                        Your subscription <b><t t-out=\"object.name\"/></b> has been cancelled.\n"
"\t\t                    </t>\n"
"\t\t                    <t t-if=\"object.state == 'close'\">\n"
"\t\t                        Your subscription <b><t t-out=\"object.name\"/></b> has been closed.\n"
"\t\t                    </t>\n"
"\t\t                    <br/>\n"
"\t\t                    <br/>\n"
"\t\t                    <t t-if=\"object.sh_end_date\">\n"
"\t\t                    \tYour plan will expire on <b><t t-out=\"object.sh_end_date\"/></b>\n"
"\t\t                    </t>\n"
"\t                        <br/>\n"
"\t                        <br/>\n"
"\t                        If you have any questions then feel free to contact us.\n"
"\t                        <br/>\n"
"\t                        Thank you...\n"
"\t                      </p>\n"
"\t                    </td>\n"
"\t                  </tbody>\n"
"\t                </table>\n"
"\t              </td>\n"
"\t            </tr>\n"
"\t\n"
"\t            <!-- FOOTER -->\n"
"\t            <tr>\n"
"\t              <td align=\"center\" style=\"min-width: 590px;\">\n"
"\t                <table width=\"590\" border=\"0\" cellpadding=\"0\" bgcolor=\"#875A7B\" style=\"min-width: 590px; background-color: rgb(135,90,123); padding: 20px; border-collapse:separate;\">\n"
"\t                  <tr>\n"
"\t                    <td valign=\"middle\" align=\"left\" style=\"color: #fff; padding-top: 10px; padding-bottom: 10px; font-size: 12px;\">\n"
"\t                      <t t-out=\"object.sh_company_id.name\"/>\n"
"\t                      <br/>\n"
"\t                      <t t-out=\"object.sh_company_id.phone or ''\"/>\n"
"\t                    </td>\n"
"\t                    <td valign=\"middle\" align=\"right\" style=\"color: #fff; padding-top: 10px; padding-bottom: 10px; font-size: 12px;\">\n"
"\t                      <t t-if=\"object.sh_company_id.email\">\n"
"\t                        <a t-attf-href=\"mailto:{{ object.sh_company_id.email }}\" style=\"text-decoration:none; color: white;\"><t t-out=\"object.sh_company_id.email\"/></a>\n"
"\t                        <br/>\n"
"\t                      </t>\n"
"\t                      <t t-if=\"object.sh_company_id.website\">\n"
"\t                        <a t-attf-href=\"{{object.sh_company_id.website}}\" style=\"text-decoration:none; color: white;\">\n"
"\t                          <t t-out=\"object.sh_company_id.website\"/>\n"
"\t                        </a>\n"
"\t                      </t>\n"
"\t                    </td>\n"
"\t                  </tr>\n"
"\t                </table>\n"
"\t              </td>\n"
"\t            </tr>\n"
"\t          </tbody>\n"
"\t        </table>\n"
"\t      </div>\n"
"\t    "
msgstr ""

